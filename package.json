{
  "name": "better-sqlite3-litesync",
  "version": "7.4.3",
  "description": "LiteSync for Node.js, using better-sqlite3",
  "homepage": "http://github.com/litesync/better-sqlite3",
  "author": "Joshua Wise <joshuathomaswise@gmail.com>",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/litesync/better-sqlite3.git"
  },
  "dependencies": {
    "bindings": "^1.5.0",
    "prebuild-install": "^6.0.1"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "cli-color": "^2.0.0",
    "fs-extra": "^9.1.0",
    "mocha": "^8.3.2",
    "nodemark": "^0.3.0",
    "prebuild": "^10.0.1"
  },
  "scripts": {
    "install": "prebuild-install || npm run build-release",
    "build-release": "node-gyp rebuild --release",
    "build-debug": "node-gyp rebuild --debug",
    "rebuild-release": "npm run lzz && npm run build-release",
    "rebuild-debug": "npm run lzz && npm run build-debug",
    "test": "mocha --exit --slow=75 --timeout=5000",
    "benchmark": "node benchmark",
    "download": "bash ./deps/download.sh",
    "lzz": "lzz -hx hpp -sx cpp -k BETTER_SQLITE3 -d -hl -sl -e ./src/better_sqlite3.lzz"
  },
  "license": "MIT",
  "keywords": [
    "litesync",
    "sqlite",
    "sqlite3",
    "database replication",
    "database",
    "sql"
  ]
}
